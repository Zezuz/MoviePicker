@model Movies

<h3>@Model.Name Information</h3>

<table class="table">
    <tr>
        <th>Movie ID</th>
        <th>Name</th>
        <th>YearOfRelease</th>
        <th>Rating</th>
        <th>Genre</th>
    </tr>
    <tr>
        <td>@Model.movieID</td>
        <td>@Model.Name</td>
        <td>@Model.YearOfRelease</td>
        <td>@Model.Rating</td>
        <td>@Model.Genre</td>
    </tr>

</table>

